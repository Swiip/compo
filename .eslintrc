{
  "extends": "airbnb-base",
  "env": {
    "browser": true
  },
  "rules": {
    "no-shadow": "off",
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "class-methods-use-this": "off",
    "import/prefer-default-export": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}
